%{
#include <stdio.h>
%}

%%

"<"[^>]+">"   { printf("HTML Tag: %s\n", yytext); }

.|\n          ;

%%

int main(int argc, char *argv[]) {
    if (argc > 1)
        yyin = fopen(argv[1], "r");
    else {
        printf("Usage: ./a.out <filename.html>\n");
        return 1;
    }

    yylex();
    return 0;
}

int yywrap() {
    return 1;
}
